    public static int binaryConverter(int binary, int counter){
        //a function to covert a decimal number into a binary and simultaneously count the number 1s in it.
        if(binary == 0){
            return 0;
        }
        if(binary%2 != 0){
            return 1 + binaryConverter(binary/2, counter+1);
        }else{
            return binaryConverter(binary/2, counter+1);
        }
    }


    public static void permute(){
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter your string: ");
        String input = scanner.nextLine();
        char[] characterArray = input.toCharArray();
        int l = 0;
        int h = characterArray.length;
        permute(characterArray, l, h);
    }

    private static void permute(char[] str, int low, int high){
        if(low == high){
            System.out.println(new String(str));
        }else{
            for(int i = low; i<high; i++){
                swap(str, low, i);
                permute(str, low+1, high);
                swap(str, low, i);
            }
        }

    }


    private static void swap(char[] str, int i, int j){
        char temp = str[i];
        str[i] = str[j];
        str[j] = temp;
    }



